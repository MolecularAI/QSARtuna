{
  "task": "optimization",
  "data": {
    "input_column": "smiles",
    "response_column": "activity",
    "training_dataset_file": "train.csv",
    "test_dataset_file": "test.csv"
  },
  "descriptors": [
    {
      "name": "ECFP",
      "parameters": {
        "radius": 3,
        "nBits": 2048
      }
    },
    { "name": "MACCS_keys",
      "parameters": {}
    }
  ],
  "settings": {
    "mode": "regression",
    "cross_validation": 5,
    "shuffle": true,
    "direction": "maximize",
    "n_trials": 20,
    "n_jobs": 1
  },
  "algorithms":
  [
    { "name": "RandomForest",
      "parameters": {
        "max_depth": {
          "low": 10,
          "high": 20
        },
        "n_estimators": {
          "low": 10,
          "high": 100
        },
        "max_features": ["auto", "sqrt", "log2"]
      }
    },
    { "name": "SVR",
      "parameters": {
        "C": {
          "low": 1e-7,
          "high": 1e7
        },
        "gamma": {
          "low": 1e-9,
          "high": 1000
        }
      }
    }
  ]
}

