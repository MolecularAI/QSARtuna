{
  "task": "optimization",
  "data": {
    "input_column": "smiles",
    "response_column": "activity",
    "training_dataset_file": "train.csv",
    "test_dataset_file": "test.csv"
  },
  "descriptors": [
    { "name": "ECFP",
      "parameters": {
        "radius": 3,
        "nBits": 2048
      }
    }
  ],
  "settings": {
    "mode": "regression",
    "cross_validation": 5,
    "shuffle": true,
    "direction": "maximize",
    "n_trials": 200,
    "n_jobs": 1
  },
  "algorithms":
  [
    { "name": "RandomForest",
      "parameters": {
        "max_depth": {
          "low": 10,
          "high": 20
        },
        "n_estimators": {
          "low": 10,
          "high": 100
        },
        "max_features": ["auto"]
      }
    },
    { "name": "SVR",
      "parameters": {
        "C": {
          "low": 1e-7,
          "high": 1e7
        },
        "gamma": {
          "low": 1e-9,
          "high": 1000
        }
      }
    },
    { "name": "Ridge",
      "parameters": {
        "alpha": {
          "low": 1,
          "high": 2
        }
      }
    },
    { "name": "Lasso",
      "parameters": {
        "alpha": {
          "low": 1,
          "high": 2
        }
      }
    },
    { "name": "XGBregressor",
      "parameters": {
        "max_depth": {
          "low": 3,
          "high": 100
        },
        "n_estimators": {
          "low": 50,
          "high": 150
        },
        "learning_rate": {
          "low": 0.05,
          "high": 0.2
        }
      }
    },
    { "name": "PLS",
      "parameters": {
        "n_components": {
          "low": 2,
          "high": 3
        }
      }
    }
  ]
}